<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pinteres</title>
  <%- include("partials/head") %>
 
 
</head>
<body>
    <%- include("partials/navigation")%>
    <div class="container p-4">
        <div class="row">
            <div class="card-columns">
                <% users.forEach(function(user){ %>
                  <div class="card" id="targeta" >
                      <img src="<%= user.path %> " class="img-fluid" alt="">
                      <div class="card-body">
                          <h5 class="card-title"><%=user.nombre%></h5>
                          <p class="card-text"><%=user.correo%></p>
                          <p class="card-text lead"><%=user.fecha%></p>
                          <a href="/image/<%= user._id%>" class="btn btn-primary btn-block">View Profile</a>
                      </div>
                  </div>
                 <% }) %>
            </div>
        </div>
    </div>
   

</body>
</html>